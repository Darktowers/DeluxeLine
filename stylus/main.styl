.error
	display none
	width: 100%;
	background: rgba(255, 59, 0, 0.92);
	color: white;
	padding: 1em;
	box-sizing: border-box;
	margin-bottom: 0.1em;
.wrap
	overflow: hidden;
	max-width: 800px;
	margin: 0 auto;
	margin-bottom: 4em;
		
body
	background blanco-tenue
	color #333
.escritorio
	display none !important
.movil
	display none !important
	
.Cabecera
	position relative
	width 100%
	overflow hidden
	border-bottom: 1px solid #058CDA;
	z-index: 3;
	&-logo
		display inline-block
		float left
		margin: .5em .5em 0em .5em;
		&Img
			width 157px
			
	
	.Menumovil-icono
		display inline-block
		float right
		width 28px
		margin: .5em .5em 0em .5em;
		.Menumovil-grid
			width: 100%;
			height: 6px;
			display: block;
			margin-bottom: 0.2em;
			background-color: azul;
.Menumovil
	display none	
	position: fixed;
	width: 100%;
	height: 100%;
	background: #0086d4;
	z-index: 4;
	top: 0;
	.cerrar
		position: absolute;
		right: 0;
		margin-right: 0.5em;
		margin-top: 0.5em;
		span
			color white
			font-size 1.5em
	.Menumovil-ul
		text-align: center
		position relative
		top 30%
		.Menumovil-item
			list-style: none;
			display: inline-block;
			width: 100%;
			.Menumovil-link		
				width: 100%;
				padding: 1em;
				box-sizing: border-box;
				position: relative;
				display: block;
				color: white;
				text-decoration: none;
				font-size: .9em;
				text-transform: uppercase;
		.lol
			width: 100%;
			padding: 1em;
			box-sizing: border-box;
			position: relative;
			display: block;
			color: white;
			text-decoration: none;
			font-size: .9em;
			text-transform: uppercase;
			
		.Iactive
			border-bottom: 2px solid white;
				
