.table
	margin-top: 1em;
	max-width: 302px;
	display: inline-block;
	table
		border-left: 0;
		border-right: 0;
		/* border: 1px solid #dce0e0; */
		border-radius: 2px;
		width: 100%;
		border-collapse: separate;
		margin-bottom: 15px;
		background white
		border-top: 3px solid #058CDA;
		
		td
			padding: 1em;
		td:nth-child(1) 
			border-right: 1px solid #dce0e0;
			border-bottom: 1px solid #dce0e0;
			border-top: 1px solid #dce0e0;
		td:nth-child(2) 
			border-bottom: 1px solid #dce0e0;
			border-top: 1px solid #dce0e0;
		td:nth-child(3)
			border-right: 1px solid #dce0e0;
			border-bottom: 1px solid #dce0e0;
		td:nth-child(4)
			border-bottom: 1px solid #dce0e0;
			 

				
