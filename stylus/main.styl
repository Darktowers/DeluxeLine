@import "normalize.styl"
@import "style.styl"
@import "nib"
@import "nib/vendor"
@import "owl.carousel"
@import "animate"
vendor-prefixes = o ms moz webkit official
azul = #058CDA
rojo = rgba(255, 47, 0, 0.95)
verde = #149152
amarillo = #FFBB00
negro = #2e2f33
blanco= #fff
blanco-tenue= #EDEFED
naranja = #FF3D00
.wrap
	overflow: hidden;
	max-width: 800px;
	margin: 0 auto;
	margin-bottom: 4em;
		
body
	background blanco-tenue

.Cabecera
	position relative
	width 100%
	overflow hidden
	border-bottom: 0.2em solid #058CDA;
	z-index: 2;
	&-logo
		display inline-block
		float left
		margin: .5em .5em 0em .5em;
		&Img
			width 130px
			
	.Menumovil
		display none
	.Menumovil-icono
		display inline-block
		float right
		width 28px
		margin: .5em .5em 0em .5em;
		.Menumovil-grid
			width: 100%;
			height: 6px;
			display: block;
			margin-bottom: 0.2em;
			background-color: azul;

.Contenido-slider
	position: relative
	top -46px
	.owl-carousel
		.item
			img
				width 100%
				height 400px
.HotelFrase
	bottom: 1em;
	z-index: 1;
	position: absolute;
	margin-left: auto;
	margin-right: auto;
	margin-top: 145px;
	left: 0;
	right: 0;
	width: 60%;
	color: azul;
	top: 0;
	text-align: center
	h1
		color blanco
	.Buscar-movil
		background-color: #fff;
		text-align: center;
		overflow: hidden;
		position: relative;
		.Bhabitacion
			color: #058CDA;
			z-index: 1;
			position: absolute;
			margin-left: auto;
			margin-right: auto;
			margin-top: 16px;
			left: 0;
			right: 0;
			width: 60%;
			font-size: 1.1em;
		.Buscar
			display: inline-block;
			padding: 1em;
			/* float: right; */
			background-color: #058cda;
			position: absolute;
			right: 0;
			top: 0;
			.icon-search
				color: white;
		.Buscar-movilInput
			position: relative;
			padding: 0.90em;
			/* right: 0; */
			width: 100%;
			text-align: center;
			padding-right: 3em;
			-webkit-box-sizing: border-box;
			box-sizing: border-box;
.Hotel
	text-align center
	.Hotel-calidad
		position: relative;
		padding: 2em 1em;
		text-align: center;
		background: #058cda;
		display: inline-block;
		max-width: 35%;
		min-height: 251px;
		h2
			font-size: 2em;
			color: blanco;
			margin: 0;
			font-weight normal
		p
			color blanco
		button
			background-color: rojo
			border: 0;
			padding: 0.5em 2em;
			color: white;
			border: 1px solid #FF3D00;
	.Hotel-calidadSlider
		display: inline-block;
		max-width: 61%;
		float: right;
		.owl-carousel-1
			.item
				img
					width 100%
					height 315px
.hotelfooter
	text-align: center;
	background-color: #058CDA;
	padding 1em
	.Hotelfooter-Links
		display inline-block
		/* box-shadow: 1px 3px 15px #058CDA; */
		color: blanco;
		text-align: left;
		border-left: 1px solid white;
		padding-left: 1em;
		.Hotelfooter-LinksItem
			list-style: none;
			margin-bottom: 0.5em;
			a
				text-decoration none
				color blanco
.HotelLove
	background-color: white;
	text-align: center;
	font-weight: bold;
	padding: 0.5em;
	box-shadow: 1px 3px 15px rgba(0, 0, 0, 0.85);
.BuscarHabitacion
	display: none;
	position: fixed;
	z-index: 2;
	top: 0;
	bottom: 0;
	background-color: rgba(0, 0, 0, 0.73);
	text-align: center;
	left: 0;
	right: 0;
	.cerrar
		display: inline-block;
		position: absolute;
		right: 0;
		top: 0;
		margin: .5em .5em 0 .5em;
		.icon-cross
			font-size: 1.5em;
			color: #058CDA;
	.BuscarHabitacion-boxForm
		display: inline-block;
		text-align: center;
		margin-top: 25%;
		.huespedes
			position: relative;
			padding: .9em;
			width: 65%;
			text-align: center;
			padding-right: 3em;
			-webkit-box-sizing: border-box;
			-moz-box-sizing: border-box;
			-webkit-box-sizing: border-box;
			box-sizing: border-box;
		.llegada
			position: relative;
			padding: .9em;
			width: 32%;
			text-align: center;
			/* padding-right: 3em; */
			-webkit-box-sizing: border-box;
			-moz-box-sizing: border-box;
			-webkit-box-sizing: border-box;
			box-sizing: border-box;
			margin-top: 0.5em;
		.salida
			position: relative;
			padding: .9em;
			width: 32%;
			text-align: center;
			/* padding-right: 3em; */
			-webkit-box-sizing: border-box;
			-moz-box-sizing: border-box;
			-webkit-box-sizing: border-box;
			box-sizing: border-box;
		.BuscarHabitacion-botonBuscar
			background-color: rgba(255,47,0,0.95);
			border: 0;
			padding: 0.5em 2em;
			color: #fff;
			border: 1px solid #ff3d00;
			width: 65%;
			margin-top: 0.5em;
			


/*Busqueda*/
.cab
	text-align center
	.Buscar-movil
		position: relative;
		padding: 0.5em 0.5em;
		/* right: 0; */
		/* width: 100%; */
		text-align: center;
		/* padding-right: 3em; */
		-webkit-box-sizing: border-box;
		box-sizing: border-box;
		margin: 0.2em 0;
		left: 14px;
.Habitaciones
	position: relative;
	.Habitacion
		transition: all 300ms ease-in-out;
		border-bottom: 3px solid #058CDA;
		background: #fff;
		position relative
		&:hover
			z-index: 2;
			box-shadow: 0 19px 38px rgba(0,0,0,0.30), 0 15px 12px rgba(0,0,0,0.22);
		.Habitacion-img
			width 100%
		.tipoHabitacion
		
			padding: 1em;
			text-align: left;
			font-weight: 100;
			top: -3px;
			position: relative;
			
			h3
				margin 0
		.precio
			position: absolute;
			bottom: 80px;
			padding: 0.5em 1em;
			color: white;
			background-color: #058CDA;
			
		.descuento
			border-radius: 50%;
			width: 50px;
			text-align: center;
			height: 50px;
			line-height: 3;
			position: absolute;
			bottom: 6px;
			right: 0;
			margin-right: 1em;
			background-color: rgb(5, 140, 218);
			color: white;
		p
			margin 0
/*Habitaciones*/
.Servicios-pri
	text-align: center;
	background-color: white;
	padding: 0.8em;
	border-bottom: 3px solid #058CDA;
	.Servicio
		display: inline-block;
		.Servicio-icono
			display: inline-block;
			img
				max-width: 40px;
		.Text
			font-size 1.2em
			margin 0
		.Number
			display block
		
				
/* Buscar escritorio*/
.BuscarEscritorio
	display none
.MenuEscritorio
	display none
				
@media screen and (min-width:817px)
	.MenuEscritorio
		display inline-block
		position absolute
		right 0
		margin-right 1em
		margin-top 1em
		.MenuEscritorio-nav
			.MenuEscritorio-item
				list-style none
				float left
				padding: 0 19px;
				.MenuEscritorio-link
					text-decoration none
					color blanco
	.Buscar-movil
		display none
	.Menumovil-icono
		display none !important
	
	.BuscarEscritorio
		position relative
		display block	
		.BuscarEscritorio-form
			text-align: center;
			.huespedes
				position: relative;
				padding: .9em;
				text-align: center;
				padding-right: 3em;
				-webkit-box-sizing: border-box;
				-moz-box-sizing: border-box;
				-webkit-box-sizing: border-box;
				box-sizing: border-box;
				color #B7B7B7
			.llegada
				position: relative;
				padding: .9em;
				text-align: center;
				/* padding-right: 3em; */
				-webkit-box-sizing: border-box;
				-moz-box-sizing: border-box;
				-webkit-box-sizing: border-box;
				box-sizing: border-box;
				margin-top: 0.5em;
			.salida
				position: relative;
				padding: .9em;
				text-align: center;
				/* padding-right: 3em; */
				-webkit-box-sizing: border-box;
				-moz-box-sizing: border-box;
				-webkit-box-sizing: border-box;
				box-sizing: border-box;
			.BuscarEscritorio-botonBuscar
				background-color:azul;
				border: 0;
				padding:.9em;
				color: #fff;
				margin-top: 0.5em;
		
				
@media screen and (max-width:817px)
	.Hotel-calidadSlider
		max-width: 480px !important;
		display: inline-block;
		/* max-width: 400px; */
		float: none !important;
		/* margin: 0 auto; */
	.Hotel-calidad
		max-width: 100% !important;
		min-height 0 !important
		
		
@media screen and (max-width:480px)
	.Hotel-calidadSlider
		max-width: 100% !important;
	
		


		
		
		
		
		
		
		
	